<script type="text/javascript">
    $.datepicker.setDefaults($.datepicker.regional[ "cs" ]);
    $('<%= selector %>').datepicker({
        dateFormat:"yy-mm-dd",
        timeFormat:'hh:mm z',
        //showTimezone: true,
        timezone: "+0100",
        separator:' ',
        changeYear:true,
        yearRange:'<%= Date.today.year - 100 %>:<%= Date.today.year + 30 %>',
        changeMonth:true
    });
</script>